{
  "easycom": {
    "^tn-(.*)": "@/tuniao-ui/components/tn-$1/tn-$1.vue"
  },
  "insetLoader": {
    "config": {
      "mToast": "<m-toast ref='toast' />"
    },
    "label": ["mToast"],
    "rootEle": "view"
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "uni-app"
      }
    }
  ],
  "subPackages": [
    {
      "root": "login",
      "pages": [
        {
          "routeName": "register",
          "path": "register",
          "style": {
            "navigationBarTitleText": "注册"
          }
        },
        {
          "routeName": "login",
          "path": "login",
          "style": {
            "navigationBarTitleText": "登录"
          }
        },
        {
          "routeName": "binduser",
          "path": "binduser",
          "style": {
            "navigationBarTitleText": ""
          }
        }
      ]
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "uniIdRouter": {},
  "condition": {
    "current": 0,
    "list": [
      {
        "name": "",
        "path": "",
        "query": ""
      }
    ]
  }
}
